{
  "name": "gatsby-starter-notion",
  "version": "1.0.1",
  "description": "Gatsby starter for notion",
  "main": "index.js",
  "scripts": {
    "start": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "rm -rf public && rm -rf .cache",
    "lint": "eslint --config .eslintrc.js 'src/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint --fix --quiet",
    "tsc": "tsc --noEmit --project tsconfig.json",
    "format": "prettier-eslint --write 'src/**/*.{js,jsx,ts,tsx}'",
    "test": "jest --bail --coverage --verbose --config jest.config.js --passWithNoTests",
    "test:watch": "jest --verbose --config jest.config.js --watch --runInBand --passWithNoTests"
  },
  "keywords": [
    "gatsby",
    "gatsby-starter",
    "gatsby-notion",
    "notion"
  ],
  "author": "wonism <yocee57@gmail.com> (https://wonism.github.io)",
  "repository": "https://github.com/wonism/gatsby-start-notion",
  "homepage": "https://github.com/wonism/gatsby-start-notion",
  "bugs": {
    "url": "https://github.com/wonism/gatsby-start-notion/issues"
  },
  "license": "MIT",
  "dependencies": {
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gatsby": "^2.18.22",
    "gatsby-plugin-force-trailing-slashes": "^1.0.4",
    "gatsby-plugin-styled-components": "^3.1.17",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "gatsby-source-notion-contents": "^1.0.13",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "jest-styled-components": "^7.0.0",
    "lint-staged": "^9.5.0",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-github-corners": "^0.2.1",
    "react-helmet": "^5.2.1",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/generator": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-literals": "^7.8.3",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^24.9.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.15",
    "@types/styled-components": "^4.4.2",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-const-enum": "0.0.5",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-gatsby": "^0.2.27",
    "enzyme": "^3.11.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.0.0",
    "gatsby-plugin-feed": "^2.3.26",
    "gatsby-plugin-google-analytics": "^2.1.33",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sharp": "^2.3.13",
    "gatsby-plugin-sitemap": "^2.2.26",
    "gatsby-plugin-typescript": "^2.1.26",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-transformer-sharp": "^2.3.12",
    "react-test-renderer": "^16.12.0",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  }
